{
	"compilerOptions": {
		"target": "es5",
		"module": "CommonJS",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "dist/src",
		"declaration": true,
		"declarationMap": false,
        "lib": ["es2017","DOM"]
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
